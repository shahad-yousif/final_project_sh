<p id="notice"><%= notice %></p>

<h1>Listing Airportlocs</h1>

<table>
  <thead>
    <tr>
      <th>Address</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @airportlocs.each do |airportloc| %>
      <tr>
        <td><%= airportloc.address %></td>
        <td><%= airportloc.latitude %></td>
        <td><%= airportloc.longitude %></td>
        <td><%= link_to 'Show', airportloc %></td>
        <td><%= link_to 'Edit', edit_airportloc_path(airportloc) %></td>
        <td><%= link_to 'Destroy', airportloc, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Airportloc', new_airportloc_path %>
